{
  "scripts": {
    "build": "tsc",
    "serve": "node build/index.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only --experimental-specifier-resolution=node src/index.ts",
    "start:db": "nodemon --exec node --loader ts-node/esm/transpile-only --experimental-specifier-resolution=node src/db/datasource/manga-datasource.ts",
    "typeorm:migration:create": "yarn typeorm migration:create src/db/migration/create-manga-table",
    "typeorm:migration:run": "yarn typeorm-ts-node-commonjs migration:run -d src/db/datasources/manga-datasource.ts"
  },
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "better-sqlite3": "^7.6.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.1",
    "node-fetch": "^3.2.8",
    "nodemon": "^2.0.19",
    "prisma": "^4.0.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.2",
    "@types/sqlite3": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module"
}
